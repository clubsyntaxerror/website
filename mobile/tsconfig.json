{
  "include": [
    "**/*.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "jsx": "react-jsx",
    "target": "ES2022",
    "strict": true,
    "checkJs": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
